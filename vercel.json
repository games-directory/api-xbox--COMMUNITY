{
  "rewrites": [
    {
      "source": "/api/image/:path*",
      "destination": "/api/image"
    },
    {
      "source": "/api/webp/:path*",
      "destination": "/api/webp"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers" : [
        {
          "key" : "Access-Control-Allow-Credential",
          "value" : "true"
        },
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        },
        {
          "key" : "Access-Control-Allow-Methods",
          "value" : "GET"
        }
      ]
    }
  ]
}
